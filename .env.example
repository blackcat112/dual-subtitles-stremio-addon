# OpenSubtitles API Configuration
# Support for multiple keys (comma-separated) for quota rotation
OPENSUBTITLES_API_KEYS=key_from_account1,key_from_account2,key_from_account3
# Fallback single key (legacy support)
OPENSUBTITLES_API_KEY=your_single_key_here

# Server Configuration
PORT=7000

# Cache Configuration (optional)
CACHE_TTL=86400
